{
  "name": "todoList-ddd",
  "version": "1.0.0",
  "main": "./src/backend/server.ts",
  "scripts": {
    "lint": "eslint ./src",
    "lint:fix": "npm run lint -- --fix",
    "webpack:server": "webpack --config webpack.server.js --watch --mode=development",
    "webpack:client": "webpack --config webpack.config.js --watch --mode=development",
    "serve": "nodemon ./dist/bundle_server.js",
    "start": "npm-run-all --parallel serve webpack:*",
    "dev": "npm-run-all --parallel serve webpack:*",
    "tsc:init": "yarn tsc --init",
    "test": "jest --passWithNoTests --no-cache --runInBand",
    "test:watch": "npm test -- --watch",
    "test:staged": "npm test -- --findRelatedTests",
    "test:ci": "npm test -- --coverage",
    "test:coveralls": "npm run test:ci && coveralls < coverage/lcov.info",
    "prepare": "husky install"
  },
  "repository": "https://github.com/rafaelcr1dev/TodoList-DDD.git",
  "author": "rafaelcr123 <rafaelcruz1@hotmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.16.8",
    "@babel/core": "^7.16.12",
    "@babel/preset-react": "^7.16.7",
    "@testing-library/jest-dom": "^5.11.4",
    "@types/jest": "^27.0.1",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/experimental-utils": "^4.31.1",
    "babel-loader": "^8.2.3",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-cypress": "^2.11.1",
    "eslint-plugin-es": "^4.1.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.21.1",
    "eslint-plugin-standard": "^5.0.0",
    "husky": "^7.0.4",
    "jest": "^27.2.0",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.0",
    "ts-jest": "^27.0.5",
    "ts-loader": "^9.2.6",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.5",
    "webpack": "^5.67.0",
    "webpack-cli": "^4.9.2",
    "webpack-livereload-plugin": "^3.0.2",
    "webpack-node-externals": "^3.0.0",
    "lint-staged": "^12.1.3",
    "git-commit-msg-linter": "^3.2.8"
  },
  "dependencies": {
    "axios": "^0.25.0",
    "express": "^4.17.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
